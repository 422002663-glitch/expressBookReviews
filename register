# Save both the POST command and its output to register
(
  echo 'curl -X POST http://localhost:3000/register -H "Content-Type: application/json" -d "{\"username\":\"newuser\",\"password\":\"Password123\"}"'
  curl -X POST http://localhost:3000/register -H "Content-Type: application/json" -d '{"username":"newuser","password":"Password123"}'
) > register
