# Save both the GET command and its output to getbooksbytitle
(
  echo 'curl -X GET "http://localhost:3000/books/title?title=Pride%20and%20Prejudice" -H "Content-Type: application/json"'
  curl -X GET "http://localhost:3000/books/title?title=Pride%20and%20Prejudice" -H "Content-Type: application/json"
) > getbooksbytitle
