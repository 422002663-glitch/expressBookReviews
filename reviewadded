# Save both the PUT command and its output to reviewadded
(
  echo 'curl -X PUT "http://localhost:3000/review/1" -H "Content-Type: application/json" -d "{\"username\":\"newuser\",\"review\":\"Great book!\"}"'
  curl -X PUT "http://localhost:3000/review/1" -H "Content-Type: application/json" -d '{"username":"newuser","review":"Great book!"}'
) > reviewadded
